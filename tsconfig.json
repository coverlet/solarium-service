{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es5",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "allowJs": true,
    "isolatedModules": true,
    "noImplicitAny": false,
    "skipLibCheck": true, 
  },
  "lib": ["es2015"],
  "exclude": ["node_modules", "dist"]
}
